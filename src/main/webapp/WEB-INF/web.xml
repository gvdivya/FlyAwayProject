<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
		xmlns="http://xmlns.jcp.org/xml/ns/javaee" 
		xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee 
							http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" 
		id="WebApp_ID" 
		version="4.0">
  <display-name>FlyAwayProject</display-name>

  <welcome-file-list>
    <welcome-file>homepage.html</welcome-file>
  </welcome-file-list> 
 
 
 <filter>
    <filter-name>registrationDetailsValidationFilter</filter-name>
    <filter-class>com.flyawayproject.RegistrationDetailsValidationFilter</filter-class>
  </filter>

  <servlet>
	<servlet-name>homepageServlet</servlet-name>
	<servlet-class>com.flyawayproject.HomePageServlet</servlet-class>
  </servlet>
  
      <servlet>
	<servlet-name>homePagejsp</servlet-name>
	<jsp-file>/homepage.html</jsp-file>
  </servlet>
  
  <servlet>
	<servlet-name>registrationservlet</servlet-name>
	<servlet-class>com.flyawayproject.RegistrationServlet</servlet-class>
  </servlet>
  
    <servlet>
	<servlet-name>flightList</servlet-name>
	<servlet-class>com.flyawayproject.FlightListServlet</servlet-class>
  </servlet>
  
    <servlet>
	<servlet-name>registrationjsp</servlet-name>
	<jsp-file>/registration.html</jsp-file>
  </servlet>
  
  <servlet>
	<servlet-name>loginservlet</servlet-name>
	<servlet-class>com.flyawayproject.LoginServlet</servlet-class>
  </servlet>
  
    <servlet>
	<servlet-name>loginjsp</servlet-name>
	<jsp-file>/login.html</jsp-file>
  </servlet>
  
      <servlet-mapping>
	<servlet-name>homePagejsp</servlet-name>
	<url-pattern>/homePagejsp</url-pattern>
  </servlet-mapping>
  
  
  <servlet-mapping>
	<servlet-name>homepageServlet</servlet-name>
	<url-pattern>/homepage</url-pattern>
  </servlet-mapping>
  
   <servlet-mapping>
	<servlet-name>registrationjsp</servlet-name>
	<url-pattern>/registrationjsp</url-pattern>
  </servlet-mapping>
  
   <servlet-mapping>
	<servlet-name>registrationservlet</servlet-name>
	<url-pattern>/registration</url-pattern>
  </servlet-mapping>
  
   <filter-mapping>
    <filter-name>registrationDetailsValidationFilter</filter-name>
    <url-pattern>/registration</url-pattern>
  </filter-mapping>
  
  <servlet-mapping>
	<servlet-name>loginjsp</servlet-name>
	<url-pattern>/loginjsp</url-pattern>
  </servlet-mapping>
  
   <servlet-mapping>
	<servlet-name>loginservlet</servlet-name>
	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  
   <servlet-mapping>
	<servlet-name>flightList</servlet-name>
	<url-pattern>/flightList</url-pattern>
  </servlet-mapping>
  
  
</web-app>